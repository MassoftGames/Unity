using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using Steamworks;

public class SteamFriendObject : MonoBehaviour
{
	
	public SteamId steamid;
	
	public void Invite()
	{
		try
		{
			SteamLobbyManager.currentLobby.InviteFriend(steamid);
		}
		catch
		{
			
		}
	}
	
	

}
